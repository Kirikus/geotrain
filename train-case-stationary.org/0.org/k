FoamFile
{
    version     2.3;
    format      ascii;
    class       volScalarField;
    location    "0.org";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{
    sides
    {
        type            slip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1.5;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    ground
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }
    top
    {
        type            slip;
    }
    ".*_wall"
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }
}


